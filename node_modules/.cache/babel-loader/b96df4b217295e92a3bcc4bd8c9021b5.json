AA,EAAA,KAAA,CAAA,OARA;AASA,kBAAA,OAAA,EAAA,KAAA,CAAA,OATA;AAUA,kBAAA,OAAA,EAAA,KAAA,CAAA;AAVA,iBAJA;AAgBA,gBAAA,OAAA,CAAA,GAAA,CAAA,UAAA;AAhBA;AAAA;AAAA,uBAkBA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,wBAAA,EAAA,UAAA,CAlBA;;AAAA;AAAA;AAAA,uBAmBA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,kBAAA,EAAA,UAAA,CAnBA;;AAAA;AAAA;AAAA,uBAoBA,KAAA,CAAA,OAAA,CAAA,IAAA,0BAAA,KAAA,CAAA,EAAA,cAAA,KAAA,CAAA,IAAA,cAAA,KAAA,CAAA,GAAA,EApBA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAuBA,gBAAA,KAAA,CAAA,uBAAA,CAAA;;AAvBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAyBA;AA7BA,GAXA;AA0CA,EAAA,QAAA,kCACA,UAAA,CAAA,CAAA,kBAAA,EAAA,iBAAA,EAAA,mBAAA,CAAA,CADA;AAGA,IAAA,eAHA,6BAGA;AAAA;;AACA,UAAA,YAAA,GAAA,KAAA,gBAAA,CAAA,IAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,MAAA,CAAA,WAAA;AAAA,OAAA,CAAA;AACA,aAAA,YAAA,CAAA,aAAA;AACA;AANA;AA1CA,CAAA","sourcesContent":["<template>\n  <div>\n    <div class=\"shim-user-menu\" ref=\"modal\">\n      <div class=\"w-100 h-100 bg-white px-3 pt-4 pb-4\">\n        <div class=\"closeIcon\">\n          <router-link to=\"/\">\n            <img class=\"logo\" src=\"../../assets/img/logo1.svg\" alt=\"\">\n          </router-link>\n          <b-icon icon=\"plus-circle\" rotate=\"45\" variant=\"secondary\" @click=\"closeDropMenu\"></b-icon>\n        </div>\n        <div class=\"w-100 mt-2\">\n          <h5 class=\"mb-4\">Новый продукт</h5>\n          <div class=\"w-100 modalAddProduct\">\n            <label for=\"ctegory_name\" class=\"label_category\">Категория</label>\n            <input type=\"text\" id=\"ctegory_name\" class=\"inputAddProduct\" :value=\"getCategoryName\">\n            <form class=\"w-100\" method=\"post\" @submit.prevent=\"createProduct\">\n              <input type=\"text\" class=\"inputAddProduct\" placeholder=\"введите наименование продукта\"\n                    v-model=\"product\">\n              <button class=\"button btn-add\" type=\"submit\">Добавить</button>\n            </form>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport {mapGetters, mapActions} from \"vuex\";\n\nexport default {\n  name: \"ModalNewProduct\",\n\n  props: ['shop', 'day', 'id', 'category_id', 'user_id', 'list_id'],\n\n  data() {\n    return {\n      product: ''\n    }\n  },\n\n  methods: {\n    closeDropMenu() {\n      this.$emit('closeModalCreateProduct')\n    },\n    async createProduct() {\n      const isProduct = this.getListProducts.filter(item => item.product_name === this.product)\n      console.log(this.product)\n      if (!isProduct.length) {\n        const newProduct = {\n          category_id: this.category_id,\n          day: this.day,\n          is_success: false,\n          shop_id: this.id,\n          shop_name: this.shop,\n          order_id: this.id,\n          category_name: this.getCategoryName,\n          product_name: this.product,\n          user_id: this.user_id,\n          list_id: this.list_id\n        }\n        console.log(newProduct)\n        try {\n          await this.$store.dispatch('createNewOrdersProduct', newProduct)\n          await this.$store.dispatch('createNewProduct', newProduct)\n          await this.$router.push(`/my_shops_shop/${this.id}/${this.shop}/${this.day}`)\n        } catch (e) {}\n      } else {\n        alert('Продукт есть в списке')\n      }\n    }\n  },\n  computed: {\n    ...mapGetters(['getAllCategories', 'getListProducts', \"getCategoryUserId\"]),\n\n    getCategoryName() {\n      const categoryName = this.getAllCategories.find(name => name.id === this.category_id)\n        return  categoryName.category_name\n    }\n  }\n}\n</script>\n\n<style scoped>\n  .modalAddProduct {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n  }\n  .btn-add {\n    width: 150px;\n    height: 56px;\n  }\n  .inputAddProduct {\n    border: 1px solid #c4c4c4;\n    background: none;\n    outline: none;\n    /*min-width: 200px;*/\n    width: 100%;\n    height: 56px;\n    border-radius: 7px;\n    padding: 0px 11px;\n    color: #242e42;\n    margin-bottom: 32px;\n  }\n  .closeIcon {\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    font-size: 32px;\n  }\n  .label_category {\n    display: flex;\n    align-self: self-start;\n  }\n</style>"],"sourceRoot":"src/components/modal"}]}